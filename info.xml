<?xml version="1.0"?>
<extension key="sgft" type="module">
  <file>sgft</file>
  <name>SGFT customisations</name>
  <description>CiviCRM Customisations for SGFT (javascript to modify contribution pages)</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Matthew Wire</author>
    <email>mjw@mjwconsult.co.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/mjwconsult/civicrm-sgft</url>
    <url desc="Support">https://mjw.pt/support/sgft</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2022-05-08</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.47</ver>
  </compatibility>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Sgft</namespace>
    <format>22.05.0</format>
    <angularModule>crmSgft</angularModule>
  </civix>
  <mixins>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
</extension>
